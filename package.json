{
  "name": "CRM",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.9.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "build-css": "node-sass ./src/scss/ -o src/ ",
    "start-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
